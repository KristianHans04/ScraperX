{
  "name": "scrapifie",
  "version": "1.0.0",
  "description": "Enterprise-grade web scraping platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:frontend": "vite build",
    "dev": "concurrently --names backend,frontend,maildev --prefix-colors blue,magenta,green \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run dev:maildev\"",
    "dev:backend": "tsx watch src/index.ts",
    "dev:frontend": "vite",
    "dev:maildev": "maildev --smtp 1025 --web 1080",
    "start": "node dist/index.js",
    "start:worker": "node dist/workers/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "typecheck:frontend": "tsc --project tsconfig.frontend.json --noEmit",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:seed": "tsx scripts/seed.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down"
  },
  "keywords": [
    "scraping",
    "web-scraping",
    "api",
    "playwright",
    "anti-detection"
  ],
  "author": "Scrapifie Team",
  "license": "MIT",
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "axios": "^1.13.5",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.1.6",
    "cheerio": "^1.0.0-rc.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^16.4.1",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.19.0",
    "express-validator": "^7.3.1",
    "fingerprint-generator": "^2.1.52",
    "fingerprint-injector": "^2.1.52",
    "helmet": "^7.2.0",
    "ioredis": "^5.3.2",
    "lucide-react": "^0.563.0",
    "maildev": "^2.2.1",
    "morgan": "^1.10.1",
    "nanoid": "^5.0.5",
    "node-schedule": "^2.1.1",
    "nodemailer": "^8.0.1",
    "pg": "^8.11.3",
    "pino": "^8.18.0",
    "playwright": "^1.41.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.13.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.4.0",
    "undici": "^6.6.1",
    "uuid": "^13.0.0",
    "vite": "^7.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/express-session": "^1.18.2",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.33",
    "@types/nodemailer": "^7.0.9",
    "@types/pg": "^8.11.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.6.1",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "eslint": "^8.56.0",
    "express": "^4.22.1",
    "nock": "^13.5.1",
    "pino-pretty": "^13.1.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
